SELECT "serviceID", "serviceName", "serviceDisplayName", "globalServiceVersion", "validFrom", "validTill", "isPublished", "serviceTypeID", "createdAt", "createdBy", "updatedAt", "updatedBy", "legacyTIPDetailID", "serviceType", status,
count(*) FROM service."Service" GROUP BY "serviceID","globalServiceVersion","legacyTIPDetailID" HAVING COUNT (*) > 1

SELECT "serviceID", "serviceName", "serviceDisplayName", "globalServiceVersion", "validFrom", "validTill", "isPublished", "serviceTypeID", "createdAt", "createdBy", "updatedAt", "updatedBy", "legacyTIPDetailID", "serviceType", status FROM service."Service" 
WHERE  "legacyTIPDetailID"='3005'
